<% s = @gen.config['bootstrap']['js'].map do |js| "  #{js['name']}: {\n    url:  '#{js['url']}',\n    hash: '#{js['hash']}'\n  }" end %>
<%= "<%" %>
libs = {
<%= s.join ",\n" %>
}

libs.each do |lib| %>
  <!-- <%%= lib[0] %> -->
  <script src='<%%= lib[1][:url] %>' integrity='<%%= lib[1][:hash] %>' crossorigin='anonymous'></script>
<%% end %>
